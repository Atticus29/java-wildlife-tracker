<!-- ranger-form.vtl -->

<form action="/" method="post">
  <label for="ranger-name">Ranger Name:</label>
  <input type="text" name="ranger-name" required>
  <label for="ranger-badge">Badge Number:</label>
  <input type="number" name="ranger-badge" required>
  <label for="ranger-email">Email Address:</label>
  <input type="email" name="ranger-email" required>
  <button type="submit" name="button" class="btn btn-success">Add Ranger</button>
</form>

<!-- ranger-form.vtl -->
